@model IEnumerable<AutoScout.Models.VehicleImage>

@{
    ViewBag.Title = "IndexPhoto";
}

<h2>IndexPhoto</h2>

<div>
    <h4>VehicleImage</h4>
    <hr />
    <table>
        <tr>
            <td>
                @foreach (var item in Model){
                     
                        var base64 = Convert.ToBase64String(item.ImageBytes);
                        var imgSrc = string.Format("data:image/png;base64,{0}", base64);
                    
                        <img src="@imgSrc" style="max-width:200px;" />
                }
            </td>
        </tr>
    </table>
</div>
<p>
    
    @Html.ActionLink("Back to List", "Index")
</p>
